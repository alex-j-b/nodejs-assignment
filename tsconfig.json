{
	"compilerOptions": {
	  "target": "esnext",
	  "module": "CommonJS",
	  "moduleResolution": "node",
	  "allowJs": true,
	  "noEmit": true,
	  "strict": false,
	  "esModuleInterop": true,
	  "pretty": true,
	  "skipLibCheck": true,
	  "resolveJsonModule": true,
	  "types": ["jest", "node"]
	},
	"exclude": ["src/node_modules"]
}
